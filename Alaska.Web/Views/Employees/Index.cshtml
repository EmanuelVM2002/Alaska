@model IEnumerable<Alaska.Web.Models.Employee>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NomEmpleado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CedEmpleado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ApeEmpleado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EdadEmpleado)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NomEmpleado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CedEmpleado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ApeEmpleado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EdadEmpleado)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
